<swiper class="swiper-list" next-margin="60rpx"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><block><swiper-item class="swiper-item" id="{{item.id}}" data-event-opts="{{[['tap',[['chooseList',['$event']]]]]}}" catchtap="__e"><view class="item-cotent"><view class="title-name">{{item.name+''}}<text class="iconfont icon-gengduo2"></text></view><view class="rank-list"><block wx:for="{{item.tracks}}" wx:for-item="ranks" wx:for-index="i" wx:key="i"><block><view class="list-item"><image class="list-image" lazy-load="{{true}}" src="{{ranks.al.picUrl}}"></image><text class="count">{{i+1}}</text><text class="count-name">{{ranks.name+''}}<text class="singer">{{"- "+ranks.ar[0].name}}</text></text></view></block></block></view></view></swiper-item></block></block></swiper>